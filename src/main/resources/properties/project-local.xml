<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>local</comment>

    <!-- mysql -->
    <entry key="base.mysql.jdbc.url">jdbc:mysql://127.0.0.1:3306/smartboard?useSSL=false&amp;useUnicode=true&amp;characterEncoding=UTF8</entry>
    <entry key="base.mysql.jdbc.username">smartuser</entry>
    <entry key="base.mysql.jdbc.password">smartuser</entry>

    <!-- hibernate -->
    <entry key="hibernate.dialect.storage_engine">innodb</entry>
    <entry key="hibernate.show_sql">true</entry>
    <entry key="hibernate.format_sql">true</entry>
    <entry key="hibernate.ejb.naming_strategy">org.hibernate.cfg.DefaultNamingStrategy</entry>
</properties>
